    #Valor de inventarios 1er y 2do semestre
    def informacion_inventarios():
        global Inv_inicial_1er_semestre, Inv_inicial_2do_semestre
        Inv_inicial_1er_semestre = float(input("Ingrese el valor del inventario inicial del primer semestre: \n"))
        Inv_inicial_2do_semestre = float(input("Ingrese el valor del inventario final del segundo semestre: \n"))

    #Costos primer y 2do semestre
    def informacion_inventarios_costos():
        global nombre_producto, Costo_1er_semestre, Costo_2do_semestre
        Costo_1er_semestre = float(input("Ingrese el costo del primer semestre: \n"))
        Costo_2do_semestre = float(input("Ingrese el costo del segundo semestre: \n"))

    #Materia prima A
    print("**** Materia prima A ****")
    informacion_inventarios()
    informacion_inventarios_costos()
