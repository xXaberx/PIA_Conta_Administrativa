    def info_producto():
        global precio_producto_semestre_1, precio_producto_semestre_2, ventas_planeadas_1, ventas_planeadas_2
        precio_producto_semestre_1 = float(input(f"Ingrese el precio de venta del primer semestre para este producto"))
        precio_producto_semestre_2 = float(input(f"Ingrese el precio de venta del segundo semestre para este producto"))
        ventas_planeadas_1 = float(input(f"Ingrese las ventas planeadas del primer semestre para este producto"))
        ventas_planeadas_2 = float(input(f"Ingrese las ventas planeadas del segundo semestre para este producto"))

    print(f"*** Producto 1: {producto1} ***")
    info_producto()
    articulo_1 = [precio_producto_semestre_1, precio_producto_semestre_2, ventas_planeadas_1, ventas_planeadas_2]
    print(f"Informacion del articulo 1: ({producto1}): ")
    print(articulo_1)
    print(f"*** Producto 2: {producto2} ***")
    info_producto()
    articulo_2 = [precio_producto_semestre_1, precio_producto_semestre_2, ventas_planeadas_1, ventas_planeadas_2]
    print(f"Informacion del articulo 2: ({producto_2}): ")
    print(articulo_2)

    print(f"*** Producto 3: {producto3} ***")
    info_producto()
    articulo_3 = [precio_producto_semestre_1, precio_producto_semestre_2, ventas_planeadas_1, ventas_planeadas_2]
    print(articulo_3)

    print(f"""***Informacion de productos***
        {producto1}: {articulo_1}
        {producto2}: {articulo_2}
        {producto3}: {articulo_3}""")


